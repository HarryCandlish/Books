{
  "name": "react-express-boilerplate",
  "version": "1.0.0",
  "description": "react express boilerplate",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HarryCandlish/React-Express-Boilerplate.git"
  },
  "author": "Harry Candlish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HarryCandlish/React-Express-Boilerplate/issues"
  },
  "dependencies": {
    "concurrently": "^4.1.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "nodemon": "^1.18.11"
  }
}
